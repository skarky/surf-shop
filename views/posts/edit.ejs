<h1>Edit new post</h1>

<form action="/posts/<%= post._id %>?_method=PUT" method="POST">
  <div><input type="text" name="title" value="<%= post.title %>"></input></div>
  <div><input type="number" name="price" value="<%= post.price %>"></input></div>
  <div><textarea name="description"><%= post.description %></textarea></div>
  <div><input type="text" name="location" value="<%= post.location %>"></input></div>
  <div><input type="submit">

</form>